{
  "expo": {
    "name": "Memot",
    "description": "Simple Memo App",
    "slug": "Memot",
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "jp.OGB4AV8tGjRO.memot",
      "buildNumber": "1.0",
    },
    "android": {
      "package":"jp.OGB4AV8tGjRO.memot",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
